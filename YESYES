-- Scripts

local function XXKCW_fake_script() -- MainGui.MainScript 
	local script = Instance.new('LocalScript', MainGui)

	local UserInputService = game:GetService("UserInputService")
	local TweenService = game:GetService("TweenService")
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	
	local LocalPlayer = Players.LocalPlayer
	local Mouse = LocalPlayer:GetMouse()
	
	local IsMobile = false
	local NotificationCooldown = false
	
	local Settings = {
		SpeedEnabled = false,
		JumpEnabled = false,
		
		Speed = 16,
		Jump = 50,
		
		NoClip = false,
		XRay = false
	}
	
	local Dragging
	local DragInput
	local DragStart
	local StartPos
	
	local MainGui = script.Parent
	local MainFrame = MainGui.MainFrame
	local FeaturesFrame = MainFrame.FeaturesFrame
	local Features = FeaturesFrame.Features
	
	local ClickSound = Instance.new("Sound", script)
	ClickSound.Name = "Click"
	ClickSound.SoundId = "rbxassetid://6895079853"
	
	local function update(input)
		local delta = input.Position - DragStart
		local dragTime = 0.04
		local SmoothDrag = {}
		SmoothDrag.Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + delta.X, StartPos.Y.Scale, StartPos.Y.Offset + delta.Y)
		local dragSmoothFunction = TweenService:Create(MainFrame, TweenInfo.new(dragTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), SmoothDrag)
		dragSmoothFunction:Play()
	end
	
	MainFrame.DragFrame.InputBegan:Connect(function(Input)
		if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
			Dragging = true
			DragStart = Input.Position
			StartPos = MainFrame.Position
			Input.Changed:Connect(function()
				if Input.UserInputState == Enum.UserInputState.End then
					Dragging = false
				end
			end)
		end
	end)
	
	MainFrame.DragFrame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and Dragging and MainFrame.Size then
			update(input)
		end
	end)
	
	if UserInputService:GetLastInputType() == Enum.UserInputType.Touch then
		IsMobile = true
	end
	
	for _,Button in pairs(MainGui:GetDescendants()) do
		if Button:IsA("GuiButton") then
			Button.MouseButton1Click:Connect(function()
				ClickSound:Play()
			end)
		end
	end
	
	MainFrame.DragFrame.CloseButton.MouseButton1Click:Connect(function()
		MainFrame.Visible = false
		
		if IsMobile then
			MainGui.MobOpenGui.Visible = true
		else
			if not NotificationCooldown then
				NotificationCooldown = true
				game.StarterGui:SetCore("SendNotification", {
					Title = "Menu Closed",
					Text = "Press Left Ctrl to open it again.",
					Duration = 10,
					Button1 = "Okay"
				})
				task.wait(3)
				NotificationCooldown = false
			end
		end
	end)
	
	UserInputService.InputBegan:Connect(function(Input, Typing)
		if not Typing and Input.KeyCode == Enum.KeyCode.LeftControl then
			MainFrame.Visible = not MainFrame.Visible
			
			if not IsMobile then
				if MainFrame.Visible == false then
					if not NotificationCooldown then
						NotificationCooldown = true
						game.StarterGui:SetCore("SendNotification", {
							Title = "Menu Closed",
							Text = "Press Left Ctrl to open it again.",
							Duration = 10,
							Button1 = "Okay"
						})
						task.wait(3)
						NotificationCooldown = false
					end
				end
			end
		end
	end)
	
	for _,Feature in pairs(Features:GetChildren()) do
		if Feature.Name == "Speed" then
			Feature.SpeedFrame.TextButton.MouseButton1Click:Connect(function()
				Settings.SpeedEnabled = not Settings.SpeedEnabled
				
				if Settings.SpeedEnabled then
					Feature.SpeedFrame.TextButton.Text = "On"
					Feature.SpeedFrame.TextButton.TextColor3 = Color3.fromRGB(0, 255, 0)
				else
					Feature.SpeedFrame.TextButton.Text = "Off"
					Feature.SpeedFrame.TextButton.TextColor3 = Color3.fromRGB(255, 0, 0)
					
					LocalPlayer.Character.Humanoid.WalkSpeed = 16
				end
			end)
			
			Features.Speed.SpeedFrame.TextBox.FocusLost:Connect(function()
				local Final = tonumber(Feature.SpeedFrame.TextBox.Text)
	
				if Final ~= nil then
					Settings.Speed = Final
	
					Feature.SpeedFrame.TextLabel.Text = "Speed: ".. Settings.Speed
				end
			end)
		elseif Feature.Name == "Jump" then
			Feature.JumpFrame.TextButton.MouseButton1Click:Connect(function()
				Settings.JumpEnabled = not Settings.JumpEnabled
	
				if Settings.JumpEnabled then
					Feature.JumpFrame.TextButton.Text = "On"
					Feature.JumpFrame.TextButton.TextColor3 = Color3.fromRGB(0, 255, 0)
				else
					Feature.JumpFrame.TextButton.Text = "Off"
					Feature.JumpFrame.TextButton.TextColor3 = Color3.fromRGB(255, 0, 0)
	
					LocalPlayer.Character.Humanoid.JumpPower = 50
				end
			end)
	
			Features.Jump.JumpFrame.TextBox.FocusLost:Connect(function()
				local Final = tonumber(Feature.JumpFrame.TextBox.Text)
	
				if Final ~= nil then
					Settings.Jump = Final
	
					Feature.JumpFrame.TextLabel.Text = "Jump: ".. Settings.Jump
				end
			end)
		elseif Feature.Name == "NoClip" then
			Feature.NoClipFrame.TextButton.MouseButton1Click:Connect(function()
				Settings.NoClip = not Settings.NoClip
				
				if Settings.NoClip == true then
					Feature.NoClipFrame.TextButton.Text = "Enabled"
					Feature.NoClipFrame.TextButton.TextColor3 = Color3.fromRGB(0, 255, 0)
				else
					Feature.NoClipFrame.TextButton.Text = "Disabled"
					Feature.NoClipFrame.TextButton.TextColor3 = Color3.fromRGB(255, 0, 0)
				end
			end)
		elseif Feature.Name == "XRay" then
			Feature.XRayFrame.TextButton.MouseButton1Click:Connect(function()
				Settings.XRay = not Settings.XRay
	
				if Settings.XRay == true then
					Feature.XRayFrame.TextButton.Text = "Enabled"
					Feature.XRayFrame.TextButton.TextColor3 = Color3.fromRGB(0, 255, 0)
					
					for _,v in pairs(workspace:GetDescendants()) do
						if v:IsA("BasePart") then
							if v:FindFirstAncestorWhichIsA("Model") and v:FindFirstAncestorWhichIsA("Model"):FindFirstChild("Humanoid") then else
								v.LocalTransparencyModifier = 0.6
							end
						end
					end
				else
					Feature.XRayFrame.TextButton.Text = "Disabled"
					Feature.XRayFrame.TextButton.TextColor3 = Color3.fromRGB(255, 0, 0)
					
					for _,v in pairs(workspace:GetDescendants()) do
						if v:IsA("BasePart") then
							if v:FindFirstAncestorWhichIsA("Model") and v:FindFirstAncestorWhichIsA("Model"):FindFirstChild("Humanoid") then else
								v.LocalTransparencyModifier = 0
							end
						end
					end
				end
			end)
		end
	end
	
	RunService.Heartbeat:Connect(function()
		if LocalPlayer.Character then
			if LocalPlayer.Character:FindFirstChild("Humanoid") then
				if Settings.SpeedEnabled then
					LocalPlayer.Character.Humanoid.WalkSpeed = Settings.Speed
				end
	
				if Settings.JumpEnabled then
					LocalPlayer.Character.Humanoid.JumpPower = Settings.Jump
				end
			end
		end
	end)
	
	RunService.Stepped:Connect(function()
		if LocalPlayer.Character then
			if LocalPlayer.Character:FindFirstChild("Humanoid") then
				if Settings.NoClip then
					for _,v in pairs(LocalPlayer.Character:GetDescendants()) do
						if v:IsA("BasePart") then
							v.CanCollide = false
						end
					end
				else
					for _,v in pairs(LocalPlayer.Character:GetDescendants()) do
						if v:IsA("BasePart") then
							if v.Name ~= "Head" and v.Name ~= "Torso" then
								v.CanCollide = false
							else
								v.CanCollide = true
							end
						end
					end
				end
			end
		end
	end)
end
coroutine.wrap(XXKCW_fake_script)()
